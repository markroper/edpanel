<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                     http://www.springframework.org/schema/beans/spring-beans.xsd">

    <import resource="deanslist.xml"/>
    <import resource="edpanel.xml"/>
    
    <bean id="dlEtlEngine" class="com.scholarscore.etl.DLETLEngine">
        <property name="deansList" ref="deansListClient"/>
        <property name="scholarScore" ref="edpanelClient"/>
    </bean>

</beans>