<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                     http://www.springframework.org/schema/beans/spring-beans.xsd">

    <import resource="edpanel.xml"/>

    <import resource="powerschool.xml"/>

    <bean
            class="org.springframework.beans.factory.config.PropertyPlaceholderConfigurer">
        <property name="location">
            <value>scholarscore.properties</value>
        </property>
    </bean>
    
    <bean id="psEtlEngine" class="com.scholarscore.etl.ETLEngine">
        <property name="powerSchool" ref="powerSchoolClient"/>
        <property name="edPanel" ref="edpanelClient"/>
    </bean>

    <import resource="deanslist.xml"/>
    <bean id="dlEtlEngine" class="com.scholarscore.etl.DLETLEngine">
        <property name="deansList" ref="deansListClient"/>
        <property name="scholarScore" ref="edpanelClient"/>
    </bean>
    
    <bean id="etlRunner" class="com.scholarscore.etl.runner.EtlRunner">
        <property name="etlEngine" ref="psEtlEngine" />
        <property name="dlEtlEngine" ref="dlEtlEngine" />
    </bean>
    
</beans>