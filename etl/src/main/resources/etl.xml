<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                     http://www.springframework.org/schema/beans/spring-beans.xsd">

    <import resource="edpanel.xml"/>
    <import resource="ps_etl.xml"/>
    <import resource="dl_etl.xml"/>

    <bean id="etlRunner" class="com.scholarscore.etl.runner.EtlRunner">
        <property name="etlEngine" ref="etlEngine" />
        <property name="dlEtlEngine" ref="dlEtlEngine" />
    </bean>
</beans>